match ; k-point 3 (z) ; GREP(static/info, '#k =   3', 42) ; 0.384999
match ; Eigenvalue  1 ; GREP(static/info, '#k =   3', 12,  1) ; -0.241550
match ; Eigenvalue  8 ; GREP(static/info, '#k =   3', 12,  8) ; -0.017705
match ; Eigenvalue 16 ; GREP(static/info, '#k =   3', 12, 16) ;  0.120734
match ; Eigenvalue 17 ; GREP(static/info, '#k =   3', 12, 17) ;  0.206635

match ; k-point 4 (x) ; GREP(static/info, '#k =   4', 18) ; 0.384999
