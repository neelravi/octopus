# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : oct-convert
Programs   : octopus; octopus_mpi
TestGroups : short-run; components
Enabled    : Yes

# Run a simple ground-sate of a carbon molecule
Input : 17-convert.01-run.inp
Precision: 0.00011
match ; Total energy       ; GREP(static/info, 'Total       =', 20) ; -145.39939707
match ; Eigenvalues        ; GREP(static/info, 'Eigenvalues =', 20) ; -36.59626127

# Convert the density from obf to dx format and check it.
Util  : oct-convert
Input : 17-convert.02-convert.inp
match ; Convert to dx (header)     ; GREP(restart/gs/density.dx, 'origin', 8, 0); -3.909937
match ; Convert to dx (line 513)   ; LINE(restart/gs/density.dx, 513, 1);    0.000000004632849
match ; Convert to dx (line 29355) ; LINE(restart/gs/density.dx, 29355, 1); -0.000000220757726
